{
  "name": "pouch-summa",
  "version": "0.0.1",
  "description": "Makes PouchDB deal better with databases synced with SummaDB.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/fiatjaf/pouch-summa.git"
  },
  "keywords": [
    "pouch",
    "pouchdb",
    "plugin",
    "summa",
    "summadb"
  ],
  "author": "Giovanni T. Parra",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/fiatjaf/pouch-summa/issues"
  },
  "scripts": {
    "test-node": "TEST_DB=testdb,http://localhost:5984/testdb istanbul test ./node_modules/mocha/bin/_mocha test/test.js",
    "test-browser": "./bin/test-browser.js",
    "test": "mocha",
    "build": "mkdirp dist && npm run browserify && npm run min",
    "browserify": "browserify index.js | derequire > dist/pouchdb.summa.js",
    "min": "uglifyjs dist/pouchdb.summa.js -mc > dist/pouchdb.summa.min.js"
  },
  "dependencies": {
    "argsarray": "0.0.1",
    "inherits": "~2.0.1",
    "lie": "^2.6.0",
    "pouchdb-wrappers": "^1.3.6"
  },
  "devDependencies": {
    "browserify": "^9.0.8",
    "chai": "^1.9.1",
    "derequire": "^2.0.0",
    "es3ify": "^0.1.4",
    "mkdirp": "^0.5.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.7-5",
    "pouchdb": "pouchdb/pouchdb",
    "request": "^2.36.0",
    "sauce-connect-launcher": "^0.4.2",
    "selenium-standalone": "^3.0.2",
    "uglify-js": "^2.4.13",
    "watchify": "^3.1.0",
    "wd": "^0.2.21"
  }
}
